{
  "name": "uikit-v2",
  "version": "0.0.0",
  "watch": {
    "test": "projects/uikit-v2-lib/src/styles/uk2-common/uk2-font.css"
  },
  "scripts": {
    "ng": "ng",
    "watch": "ng build --project uikit-v2-lib --watch",
    "buildLib": "ng build --project uikit-v2-lib",
    "buildLib:prod": "ng build --project uikit-v2-lib --configuration=production",
    "testLib:local": "ng test uikit-v2-lib --karma-config=karma.dev.conf.js",
    "testLib:coverage": "ng test uikit-v2-lib --watch=false --code-coverage",
    "lintLib": "ng lint uikit-v2-lib",
    "lintLib:fix": "ng lint uikit-v2-lib --fix",
    "buildDocsDetailed": "ng build --project uikit-v2-docs --named-chunks --output-hashing none",
    "testDocs": "ng test uikit-v2-docs --code-coverage",
    "startDocs": "ng serve -o",
    "lintDocs": "ng lint uikit-v2-docs",
    "lintDocs:fix": "ng lint uikit-v2-docs --fix",
    "lint:all": "ng lint",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run uikit-v2-docs:storybook",
    "build-storybook": "npm run docs:json && ng run uikit-v2-docs:build-storybook",
    "lint": "ng lint",
    "generate-font-mixins": "sass projects/uikit-v2-lib/src/styles/uk2-common/uk2-font/uk2-font.scss  projects/uikit-v2-lib/src/styles/uk2-common/uk2-font/uk2-font.css &&  node projects/uikit-v2-lib/src/scripts/generate-font-mixins.js"
  },
